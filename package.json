{
  "name": "bolt-new",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build && npm run typecheck",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:format": "npm run lint -- --format",
    "typecheck": "tsc --noEmit",
    "preview": "npm run build && npm run start"
  },
  "dependencies": {
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/vercel": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "@nanostores/react": "^0.7.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-toastify": "^10.0.4",
    "remix-island": "^0.1.2",
    "isbot": "^3.6.8"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@unocss/reset": "^0.58.3",
    "typescript": "^5.7.3",
    "vite": "^5.4.14",
    "vitest": "^1.0.0",
    "eslint": "^8.56.0",
    "sass": "^1.70.0"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
