{
  "name": "gobeze-ai",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build && npm run typecheck",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:format": "npm run lint -- --format",
    "typecheck": "tsc --noEmit",
    "preview": "npm run build && npm run start"
  },
  "dependencies": {
    "@atproto/api": "^0.9.6",
    "@atproto/lexicon": "^0.3.3",
    "@atproto/repo": "^0.3.6",
    "@google-cloud/error-reporting": "^3.0.5",
    "@google-cloud/logging": "^11.2.0",
    "@google-cloud/monitoring": "^4.1.0",
    "@nanostores/react": "^0.7.1",
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "ioredis": "^5.3.2",
    "isbot": "^3.6.8",
    "limiter": "^2.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-toastify": "^10.0.4",
    "remix-island": "^0.1.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.3",
    "@types/ioredis": "^5.0.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@unocss/reset": "^0.58.3",
    "eslint": "^8.56.0",
    "sass": "^1.70.0",
    "typescript": "^5.7.3",
    "unocss": "^65.4.3",
    "vite": "^5.4.14",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-optimize-css-modules": "^1.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
